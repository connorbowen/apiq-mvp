{
  "name": "apiq-mvp",
  "version": "1.0.0",
  "description": "Multi-API Orchestrator Webapp - AI-powered workflow automation across multiple APIs",
  "private": true,
  "scripts": {
    "dev": "./scripts/smart-dev.sh",
    "dev:direct": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "cross-env DOTENV_CONFIG_PATH=.env.test jest --runInBand --max-old-space-size=4096 --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:unit": "jest tests/unit --detectOpenHandles",
    "test:dashboard": "jest tests/unit/app/dashboard/page.test.tsx --runInBand --max-old-space-size=4096 --detectOpenHandles --logHeapUsage",
    "test:integration": "cross-env DOTENV_CONFIG_PATH=.env.test jest --config=jest.integration.config.js --detectOpenHandles",
    "test:integration:debug": "cross-env DOTENV_CONFIG_PATH=.env.test jest --config=jest.integration.config.js --detectOpenHandles --runInBand --verbose",
    "test:integration:dotenv": "dotenv -e .env.test -- jest --config=jest.integration.config.js",
    "test:integration:fast": "cross-env DOTENV_CONFIG_PATH=.env.test jest --config=jest.integration.config.js --detectOpenHandles --maxWorkers=4 --testTimeout=8000",
    "test:integration:core": "cross-env DOTENV_CONFIG_PATH=.env.test jest --config=jest.integration.config.js --detectOpenHandles tests/integration/api/auth.test.ts tests/integration/api/health.test.ts tests/integration/api/workflows.test.ts tests/integration/api/secrets.integration.test.ts",
    "test:integration:auth": "cross-env DOTENV_CONFIG_PATH=.env.test jest --config=jest.integration.config.js --detectOpenHandles tests/integration/api/auth.test.ts tests/integration/api/auth-flow.test.ts tests/integration/api/registration.test.ts",
    "test:integration:oauth2": "cross-env DOTENV_CONFIG_PATH=.env.test jest --config=jest.integration.config.js --detectOpenHandles tests/integration/api/oauth2.test.ts tests/integration/api/oauth2-google.test.ts tests/integration/api/oauth2-slack.test.ts tests/integration/api/oauth2-github.test.ts",
    "test:integration:workflows": "cross-env DOTENV_CONFIG_PATH=.env.test jest --config=jest.integration.config.js --detectOpenHandles tests/integration/api/workflows.test.ts tests/integration/queue/queueService.integration.test.ts",
    "test:integration:secrets": "cross-env DOTENV_CONFIG_PATH=.env.test jest --config=jest.integration.config.js --detectOpenHandles tests/integration/api/secrets.integration.test.ts tests/integration/lib/secrets/secretsVault.integration.test.ts",
    "test:oauth2": "dotenv -e .env.test -- jest tests/integration/api/oauth2.test.ts --detectOpenHandles",
    "test:e2e": "./scripts/kill-port-3000.sh && playwright test",
    "test:e2e:all-browsers": "CI=true playwright test",
    "test:e2e:auth": "./scripts/kill-port-3000.sh && playwright test tests/e2e/auth/",
    "test:e2e:workflows": "./scripts/kill-port-3000.sh && playwright test tests/e2e/workflows/",
    "test:e2e:connections": "./scripts/kill-port-3000.sh && playwright test tests/e2e/connections/",
    "test:e2e:ui": "./scripts/kill-port-3000.sh && playwright test tests/e2e/ui/",
    "test:e2e:ui-fast": "playwright test --config=playwright.ui.config.ts",
    "test:e2e:ui-script": "./scripts/run-ui-tests.sh",
    "test:e2e:ui-critical": "playwright test --config=playwright.critical.config.ts",
    "test:e2e:ui-interactive": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:with-server": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && playwright test\" --kill-others",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e:with-server",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "startup": "./scripts/startup.sh",
    "db:test": "tsx scripts/test-db.ts",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "db:setup": "npm run db:migrate && npm run db:generate",
    "rotate-secrets": "node scripts/rotate-secrets.js",
    "check:no-mock-data": "grep -r -E 'test-user-123|test-user-456|demo-key|demo-token|fake API|mock.*api|create-test-user' . --exclude-dir=tests --exclude-dir=node_modules --exclude-dir=.git --exclude=package-lock.json --exclude=package.json --exclude=.husky || exit 0",
    "test:e2e:current": "./scripts/kill-port-3000.sh && playwright test tests/e2e/auth/authentication-session.test.ts tests/e2e/auth/oauth2-google-signin.test.ts tests/e2e/ui/critical-ui.test.ts tests/e2e/ui/basic-navigation.test.ts --grep-invert='oauth2|advanced|complex'",
    "test:e2e:p0": "./scripts/kill-port-3000.sh && playwright test tests/e2e/auth/authentication-session.test.ts tests/e2e/auth/registration-verification.test.ts tests/e2e/auth/password-reset.test.ts tests/e2e/connections/ tests/e2e/workflow-engine/workflow-management.test.ts tests/e2e/security/",
    "test:e2e:p0:fast": "DISABLE_RATE_LIMITING=true ./scripts/kill-port-3000.sh && playwright test tests/e2e/auth/authentication-session.test.ts tests/e2e/auth/registration-verification.test.ts tests/e2e/auth/password-reset.test.ts tests/e2e/connections/ tests/e2e/workflow-engine/workflow-management.test.ts tests/e2e/security/",
    "test:e2e:p1": "./scripts/kill-port-3000.sh && playwright test tests/e2e/ui/ tests/e2e/workflow-engine/step-runner-engine.test.ts",
    "test:e2e:p2": "./scripts/kill-port-3000.sh && playwright test tests/e2e/security/",
    "test:e2e:p3": "echo 'P3 features not yet implemented - no E2E tests to run'",
    "test:e2e:auth-area": "./scripts/kill-port-3000.sh && playwright test tests/e2e/auth/authentication-session.test.ts tests/e2e/auth/registration-verification.test.ts tests/e2e/auth/password-reset.test.ts tests/e2e/auth/oauth2-authentication.test.ts",
    "test:e2e:auth-area:fast": "DISABLE_RATE_LIMITING=true ./scripts/kill-port-3000.sh && playwright test tests/e2e/auth/authentication-session.test.ts tests/e2e/auth/registration-verification.test.ts tests/e2e/auth/password-reset.test.ts tests/e2e/auth/oauth2-authentication.test.ts",
    "test:e2e:connections-area": "./scripts/kill-port-3000.sh && playwright test tests/e2e/connections/ --grep-invert='oauth2'",
    "test:e2e:workflow-area": "./scripts/kill-port-3000.sh && playwright test tests/e2e/workflow-engine/workflow-management.test.ts tests/e2e/workflow-engine/step-runner-engine.test.ts --grep-invert='advanced|complex|data-flow|conditional'",
    "test:e2e:ui-area": "./scripts/kill-port-3000.sh && playwright test tests/e2e/ui/",
    "test:e2e:security-area": "./scripts/kill-port-3000.sh && playwright test tests/e2e/security/",
    "test:e2e:security-area:fast": "DISABLE_RATE_LIMITING=true ./scripts/kill-port-3000.sh && playwright test tests/e2e/security/",
    "test:e2e:fast": "DISABLE_RATE_LIMITING=true ./scripts/kill-port-3000.sh && playwright test tests/e2e/ui/critical-ui.test.ts tests/e2e/auth/authentication-session.test.ts --timeout=10000",
    "test:e2e:smoke": "./scripts/kill-port-3000.sh && playwright test tests/e2e/ui/critical-ui.test.ts tests/e2e/auth/authentication-session.test.ts tests/e2e/security/secrets-vault.test.ts",
    "test:e2e:core": "./scripts/kill-port-3000.sh && playwright test tests/e2e/auth/authentication-session.test.ts tests/e2e/auth/registration-verification.test.ts tests/e2e/auth/password-reset.test.ts tests/e2e/ui/critical-ui.test.ts tests/e2e/ui/basic-navigation.test.ts tests/e2e/security/secrets-vault.test.ts tests/e2e/workflow-engine/workflow-management.test.ts",
    "test:e2e:core:fast": "DISABLE_RATE_LIMITING=true ./scripts/kill-port-3000.sh && playwright test tests/e2e/auth/authentication-session.test.ts tests/e2e/auth/registration-verification.test.ts tests/e2e/auth/password-reset.test.ts tests/e2e/ui/critical-ui.test.ts tests/e2e/ui/basic-navigation.test.ts tests/e2e/security/secrets-vault.test.ts tests/e2e/workflow-engine/workflow-management.test.ts",
    "test:e2e:debug": "DISABLE_RATE_LIMITING=true playwright test --headed --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:setup-smtp": "./scripts/setup-e2e-smtp.sh",
    "check:server": "node scripts/check-server-health.js",
    "test:e2e:safe": "npm run check:server && playwright test"
  },
  "keywords": [
    "api",
    "orchestrator",
    "workflow",
    "automation",
    "ai",
    "openai",
    "multi-api",
    "nextjs"
  ],
  "author": "APIQ Team",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.0.0",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.10.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^6.4.17",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.0.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.30",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "openai": "^4.20.1",
    "pg-boss": "^10.3.2",
    "react": "^18",
    "react-dom": "^18",
    "swagger-parser": "^10.0.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.15.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.0.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^3.3.2",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.1",
    "prisma": "^6.10.1",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "wait-on": "^8.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
